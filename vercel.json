{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "src/views/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      }
    ]
  }